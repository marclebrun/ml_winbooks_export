<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record
            id="ml_winbooks_export_wizard"
            model="ir.ui.view">

            <field name="name">Winbooks Export</field>
            <field name="model">ml.winbooks.export</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Accounting Winbooks Export (ML)">
            		<field name="company_id" invisible="1"/>
            		<group col="4">
                		<field name="date_from"/>
                		<field name="date_to"/>
            		</group>
                    <separator string ="Report" colspan="4"/>
                	<group colspan="4">
                		<field name="export_filename" invisible="1"/>
                    	<field name="data" filename="export_filename"/>
                	</group>
                    <footer>
                        <button name="action_manual_export_invoice_entries" string="Invoice Entries" type="object" class="oe_highlight"/>
                        <button string="Close" special="cancel" />
                    </footer>
                </form>
            </field>

         </record>

    </data>
</odoo>
